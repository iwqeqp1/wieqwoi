local Atlas = loadstring(game:HttpGet("https://raw.githubusercontent.com/DevVy17/ieuwop/main/--%5B%5B.txt"))()

local test = Atlas.new({
    Name = "AimAX BETA v1 | devy17";
    ConfigFolder = "Aimaxscriptbeta";
    Credit = "Made by DevVy!";
    Color = Color3.fromRGB(164, 53, 90);
    FullName = "AimAX BETA";
    UseLoader = true;
    Bind = "V";
    CheckKey = function(e) -- this can be nil to disable key checking
        return e == "1"
    end;
    Discord = "https://discord.gg/833aWTp8Bt"
})

local p1 = test:CreatePage("Main page")
local p2 = test:CreatePage("Esp page")
local p3 = test:CreatePage("Tools page")
local p4 = test:CreatePage("Fun page")

local s1 = p1:CreateSection("Camlock")
local s2 = p1:CreateSection("Lock")
local s3 = p1:CreateSection("AntiLock")
local s4 = p2:CreateSection("Player Esp")
local s5 = p3:CreateSection("Tools")
local s6 = p4:CreateSection("Fun scripts")

s1:CreateButton({
    Name = "Cam Lock"; -- required: name of element
    Callback = function() -- required: function to be called when button is pressed
        test:Notify({
            Title = "Camlock activated";
            Content = "Key:Q DisableKey:Q/P"
        })
        loadstring(game:HttpGet('https://raw.githubusercontent.com/DevVy17/camlock/main/camlock.txt'))()
    end
})

